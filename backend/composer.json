{
  "name": "itpoc/wwr",
  "description": "Wake Word Recorder backend",
  "type": "project",
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Damien JARRY",
      "email": "dev.damienjarry@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "dflydev/fig-cookies": "^2.0",
    "doctrine/migrations": "^2.2",
    "doctrine/orm": "^2.7",
    "facebook/graph-sdk": "^5.7",
    "firebase/php-jwt": "^5.0",
    "google/apiclient": "^2.0",
    "guzzlehttp/guzzle": "^6.5",
    "jms/serializer": "^3.4",
    "php-di/php-di": "^6.0",
    "ramsey/uuid-doctrine": "^1.6",
    "selective/config": "^0.1.1",
    "slim/psr7": "^1.0",
    "slim/slim": "^4.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true,
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
